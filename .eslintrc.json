{
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "rules": {
    "react/prefer-stateless-function": [2, { "ignorePureComponents": true }],
    "comma-dangle": ["error", "only-multiline"],
    "react/jsx-filename-extension": ["always"],
    "react/forbid-prop-types": ["always"],
    "react/prefer-stateless-function": ["never"]
  }
}
